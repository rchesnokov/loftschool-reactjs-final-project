printWidth: 80
proseWrap: always
singleQuote: true
semi: false
bracketSpacing: true
trailingComma: "all"
parser: babylon
overrides:
- files:
  - '*.ts'
  - '*.tsx'
  options:
    parser: typescript
- files: '*.md'
  options:
    parser: markdown
- files: '*.json'
  options:
    parser: json
    printWidth: 80
- files: '*.css'
  options:
    parser: css
